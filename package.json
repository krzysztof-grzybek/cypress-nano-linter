{
  "name": "cypress-nano-linter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "dev": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krzysztof-grzybek/cypress-nano-linter.git"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@types/node": "^10.5.8",
    "tslint": "^5.10.0",
    "tsutils": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/**/*.spec.(ts|tsx)"
    ],
    "testEnvironment": "node"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krzysztof-grzybek/cypress-nano-linter/issues"
  },
  "homepage": "https://github.com/krzysztof-grzybek/cypress-nano-linter"
}
